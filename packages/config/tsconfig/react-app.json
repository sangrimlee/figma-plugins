{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Application",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ES6",
    "module": "ESNext",
    "resolveJsonModule": true,
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "noEmit": true
  }
}
